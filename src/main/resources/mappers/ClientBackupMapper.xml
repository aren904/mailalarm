<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.infocore.mapper.ClientBackupMapper">
    <resultMap id="clientBackupMap" type="cn.infocore.entity.ClientBackup" >
        <id column="id" property="id"  />
        <result column="uuid" property="uuid" />
        <result column="user_id" property="userId"  />
        <result column="client_id" property="ClientId"  />
    	<result column="data_ark_dr_id" property="dataArkDrId"  />
    	<result column="type" property="type"  />
    	<result column="exceptions" property="exceptions"  />
    	<result column="size" property="size"  />
    	<result column="preoccupation_size" property="preoccupationSize"  />
    	<result column="name" property="name"  />
    	<result column="db_type" property="dbType"  />
    	<result column="operating_system" property="operatingSystem"  />
    	<result column="path" property="path"  />
    	<result column="database_type" property="databaseType"  />
    </resultMap>
    
    <update id="updateVirtualMachineByUuid">
		update client_backup set name=#{name},path=#{path},exceptions=#{exception},operating_system=#{operatingSystem} where uuid=#{uuid}
	</update>
	
</mapper>
